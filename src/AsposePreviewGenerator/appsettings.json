{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/previewgenerator-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "sensenet": {
    "AsposePreviewGenerator": {
      "Upload": {
        "ChunkSize": 10485760
      },
      "ImageGeneration": {
        "PreviewResolution": 300,
        "CheckLicense": true
      },
      "Applications": []
    }
  }
}